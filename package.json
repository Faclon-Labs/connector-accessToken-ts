{
  "name": "iosense-sdk",
  "version": "1.0.0",
  "description": "Official TypeScript SDK for IOSense platform APIs, providing seamless access to device management, user operations, authentication, and multi-protocol pub/sub messaging systems.",
  "keywords": ["iosense", "iot", "typescript", "sdk", "mqtt", "kafka", "redis", "postgresql", "device-management", "pubsub"],
  "author": "FaclonLabs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Faclon-Labs/connector-accessToken-ts.git"
  },
  "homepage": "https://github.com/Faclon-Labs/connector-accessToken-ts#readme",
  "bugs": {
    "url": "https://github.com/Faclon-Labs/connector-accessToken-ts/issues"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./browser": {
      "import": "./dist/browser/index.js",
      "types": "./dist/browser/index.d.ts"
    },
    "./node": {
      "import": "./dist/node/index.js", 
      "types": "./dist/node/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run build:core && npm run build:browser && npm run build:node && npm run build:main",
    "build:core": "tsc -p tsconfig.core.json",
    "build:browser": "tsc -p tsconfig.browser.json", 
    "build:node": "tsc -p tsconfig.node.json",
    "build:main": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "test": "node --loader ts-node/esm test.ts",
    "test:calibration": "npx tsx test/test_data_by_time_range_calibration.ts",
    "test:stet": "npx tsx test/test_get_data_by_stet.ts",
    "test:limited": "npx tsx test/test_get_limited_data.ts",
    "test:monthly": "npx tsx test/test_get_monthly_consumption.ts",
    "test:entities": "npx tsx test/test_load_entities_gen.ts",
    "test:entity-by-id": "npx tsx test/test_load_entities_gen_by_id.ts",
    "test:entities-paginated": "npx tsx test/test_load_entities_gen_paginated.ts"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.7",
    "@types/pg": "^8.15.5",
    "rimraf": "^6.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "axios": "^1.10.0"
  },
  "optionalDependencies": {
    "@types/mqtt": "^2.5.0",
    "amqplib": "^0.10.8",
    "ioredis": "^5.6.1",
    "kafkajs": "^2.2.4",
    "mongo": "^0.1.0",
    "mqtt": "^5.13.1",
    "pg": "^8.16.3"
  },
  "type": "module",
  "sideEffects": false
}
